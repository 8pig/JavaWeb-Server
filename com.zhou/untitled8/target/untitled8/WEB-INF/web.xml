<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>CharchateFilter</filter-name>
        <filter-class>com.zhou.web.CharchateFilter</filter-class>
    </filter>

    <servlet>
        <servlet-name>regUser</servlet-name>
        <servlet-class>com.zhou.web.regUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>regUser</servlet-name>
        <url-pattern>/regUser</url-pattern>
    </servlet-mapping>


</web-app>
